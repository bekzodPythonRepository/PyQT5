from PyQt5 import QtCore

# Добавьте в системные переменные PATH /путь к интерпретатору Python/Lib//site-packages//PyQt5//Qt5//plugins/

p = QtCore.QPoint()
print(p.isNull()) # True проверка обеих координат на равенство 0
p.setX(10) # установка координаты X
p.setY(88) # установка координаты Y
print(p.x(), p.y()) # 10 88 
print(QtCore.QPoint(10, 88).manhattanLength()) # 98 возвращение суммы коррдинат X и Y
